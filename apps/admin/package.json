    "dereference": "esrun src/scripts/dereference.ts",
    "dev": "yarn structure && vite",
    "dump": "yarn dump:schema && yarn dump:data && yarn dump:roles",
    "dump:data": "supabase db dump -f supabase/seed.sql --data-only",
    "dump:roles": "supabase db dump -f supabase/roles.sql --role-only",
    "dump:schema": "supabase db dump -f supabase/schema.sql",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "gen:definitions": "yarn schema",
    "gen:openapi": "npx openapi-typescript openapi.json --output schemas/",
    "gen:resources": "esrun src/scripts/genResources.ts",
    "gen:resources:employees": "esrun src/scripts/genEmployeesResources.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --fix --max-warnings 0",
    "postgres:dump": "pg_dump -h db.eqwtcznujnbxfqnbjrsx.supabase.co -p 5432 -d postgres -U postgres -p 5432 -f supabase_pg_dump.sql",
    "preview": "vite preview",
    "schema": "esrun src/scripts/genSchema.ts",
    "seed": "esrun src/scripts/seed.ts",
    "structure": "esrun src/scripts/genStructure.ts",
    "tasks:graph": "turbo run dev build lint --graph=tasks_graph.mermaid",
    "tsc": "tsc",
    "update": "yarn && rm src/@react-admin/ra** -f -r -d && sh update.sh"
